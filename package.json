{
  "name": "bugcatcher-api",
  "version": "1.0.21",
  "description": "Client module for communicating with the Faster Than Light BugCatcher backend API.",
  "main": "dist/bugcatcher-api.cjs.js",
  "module": "dist/bugcatcher-api.esm.js",
  "engines": {
    "node": ">=11"
  },
  "scripts": {
    "build": "npm-run-all -p build:*",
    "build:cjs": "rollup index.js -c rollup.config.js --file dist/bugcatcher-api.cjs.js --format cjs",
    "build:esm": "rollup index.js -c rollup.config.js --file dist/bugcatcher-api.esm.js --format esm",
    "prepublishOnly": "npm run build",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "repository": {
    "type": "git",
    "url": "git+ssh://git@github.com/faster-than-light/bugcatcher-api.git"
  },
  "keywords": [
    "solidity",
    "truffle",
    "smart contracts",
    "static analysis"
  ],
  "author": "Reuven Etzion",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/faster-than-light/bugcatcher-api/issues"
  },
  "homepage": "https://github.com/faster-than-light/bugcatcher-api#readme",
  "dependencies": {
    "axios": "^0.18.0"
  },
  "devDependencies": {
    "npm-run-all": "^4.1.3",
    "proxyquire": "^2.0.1",
    "rollup": "^1.1.2",
    "rollup-plugin-commonjs": "^9.2.0",
    "rollup-plugin-node-resolve": "^4.0.0"
  }
}
